<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY uo "http://purl.org/ontology/UniOn#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://purl.org/ontology/UniOn#"
     xml:base="http://purl.org/ontology/UniOn"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:uo="http://purl.org/ontology/UniOn#">
    <owl:Ontology rdf:about="http://purl.org/ontology/UniOn">
        <rdfs:comment>UniOn is an ontology representing Universities educational offer. It includes information to describe academic fields, degree programs, access methods and requirements, educational activities, titles and qualifications, location, organization and more</rdfs:comment>
        <owl:versionIRI rdf:resource="http://purl.org/ontology/UniOn/09-2022"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/ontology/UniOn#delivers -->

    <owl:ObjectProperty rdf:about="&uo;delivers">
        <rdfs:comment>A property that relates an italian public university to the degree programmes it delivers.</rdfs:comment>
        <rdfs:range rdf:resource="&uo;DegreeProgramme"/>
        <rdfs:domain rdf:resource="&uo;ItalianPublicUniversity"/>
        <owl:inverseOf rdf:resource="&uo;isDeliveredBy"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#hasAcademicField -->

    <owl:ObjectProperty rdf:about="&uo;hasAcademicField">
        <rdfs:comment>A property linking the degree programme to the academic field to which it belongs.</rdfs:comment>
        <rdfs:range rdf:resource="&uo;AcademicField"/>
        <rdfs:domain rdf:resource="&uo;DegreeProgramme"/>
        <owl:inverseOf rdf:resource="&uo;isAcademicFieldOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#hasAccessType -->

    <owl:ObjectProperty rdf:about="&uo;hasAccessType">
        <rdfs:comment>A property relating the degree programme to the modality of access it requires.</rdfs:comment>
        <rdfs:range rdf:resource="&uo;AccessType"/>
        <rdfs:domain rdf:resource="&uo;DegreeProgramme"/>
        <owl:inverseOf rdf:resource="&uo;isAccessTypeOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#hasAssessmentMethod -->

    <owl:ObjectProperty rdf:about="&uo;hasAssessmentMethod">
        <rdfs:comment>A property that relates an educational activity to a particular evaluation modality. Example of an assessment method include an oral or  writtem exam, the subsmission of a project or an eligibility.</rdfs:comment>
        <rdfs:range rdf:resource="&uo;AssessmentMethod"/>
        <rdfs:domain rdf:resource="&uo;EducationalActivity"/>
        <owl:inverseOf rdf:resource="&uo;isAssessmentMethodOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#hasCampus -->

    <owl:ObjectProperty rdf:about="&uo;hasCampus">
        <rdfs:comment>A property that associates the degree programme to the particular place where the university campus is located.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;DegreeProgramme"/>
        <owl:inverseOf rdf:resource="&uo;isCampusOf"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#hasCurriculum -->

    <owl:ObjectProperty rdf:about="&uo;hasCurriculum">
        <rdfs:comment>A property linking a degree programme to the curricula it offers.</rdfs:comment>
        <rdfs:range rdf:resource="&uo;Curriculum"/>
        <rdfs:domain rdf:resource="&uo;DegreeProgramme"/>
        <owl:inverseOf rdf:resource="&uo;isCurriculumOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#hasEducationalActivity -->

    <owl:ObjectProperty rdf:about="&uo;hasEducationalActivity">
        <rdfs:comment>A property that associates a degree programme to the educational activities it delivers.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;DegreeProgramme"/>
        <rdfs:range rdf:resource="&uo;EducationalActivity"/>
        <owl:inverseOf rdf:resource="&uo;isEducationalActivityOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#hasPlaceOfTeaching -->

    <owl:ObjectProperty rdf:about="&uo;hasPlaceOfTeaching">
        <rdfs:comment>A property that relates the degree programme to the place where its educational activities take place.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;DegreeProgramme"/>
        <owl:inverseOf rdf:resource="&uo;isPlaceOfTeachingOf"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#isAcademicFieldOf -->

    <owl:ObjectProperty rdf:about="&uo;isAcademicFieldOf">
        <rdfs:comment>A property describing the academic field to which a degree programme belongs.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;AcademicField"/>
        <rdfs:range rdf:resource="&uo;DegreeProgramme"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#isAccessTypeOf -->

    <owl:ObjectProperty rdf:about="&uo;isAccessTypeOf">
        <rdfs:comment>A property indicating the modality of access a degree programme requires.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;AccessType"/>
        <rdfs:range rdf:resource="&uo;DegreeProgramme"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#isAssessmentMethodOf -->

    <owl:ObjectProperty rdf:about="&uo;isAssessmentMethodOf">
        <rdfs:comment>A property expressing  the evaluation modality of an educational activity. Example of an assessment method include an oral or  writtem exam, the subsmission of a project or an eligibility.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;AssessmentMethod"/>
        <rdfs:range rdf:resource="&uo;EducationalActivity"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#isCampusOf -->

    <owl:ObjectProperty rdf:about="&uo;isCampusOf">
        <rdfs:comment>A property indicating the place where the campus of a particular degree programme is located.</rdfs:comment>
        <rdfs:range rdf:resource="&uo;DegreeProgramme"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#isCurriculumOf -->

    <owl:ObjectProperty rdf:about="&uo;isCurriculumOf">
        <rdfs:comment>A property that  indicates the curricula offered by the same degree programme.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;Curriculum"/>
        <rdfs:range rdf:resource="&uo;DegreeProgramme"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#isDeliveredBy -->

    <owl:ObjectProperty rdf:about="&uo;isDeliveredBy">
        <rdfs:comment>A property indicating the degree programmes delivered by an italian public university.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;DegreeProgramme"/>
        <rdfs:range rdf:resource="&uo;ItalianPublicUniversity"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#isEducationalActivityOf -->

    <owl:ObjectProperty rdf:about="&uo;isEducationalActivityOf">
        <rdfs:comment>A property indicating the educational activity delivered by a degree programme.</rdfs:comment>
        <rdfs:range rdf:resource="&uo;DegreeProgramme"/>
        <rdfs:domain rdf:resource="&uo;EducationalActivity"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#isManagedBy -->

    <owl:ObjectProperty rdf:about="&uo;isManagedBy">
        <rdfs:comment>A property indicating the university department managed by a public italian university.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;DegreeProgramme"/>
        <rdfs:range rdf:resource="&uo;Department"/>
        <owl:inverseOf rdf:resource="&uo;manages"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#isOfferedBy -->

    <owl:ObjectProperty rdf:about="&uo;isOfferedBy">
        <rdfs:comment>Superproperty that joins an educational activity to a particular italian public university via the degree programme.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;EducationalActivity"/>
        <rdfs:range rdf:resource="&uo;ItalianPublicUniversity"/>
        <owl:inverseOf rdf:resource="&uo;offers"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#isPlaceOfTeachingOf -->

    <owl:ObjectProperty rdf:about="&uo;isPlaceOfTeachingOf">
        <rdfs:comment>A property that indicates the location where the degree programme&apos;s educational acitvities take place.</rdfs:comment>
        <rdfs:range rdf:resource="&uo;DegreeProgramme"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#isReleasedBy -->

    <owl:ObjectProperty rdf:about="&uo;isReleasedBy">
        <rdfs:comment>A property expressing the official qualification released to the student after the completion of a degree programme.</rdfs:comment>
        <rdfs:range rdf:resource="&uo;DegreeProgramme"/>
        <rdfs:domain rdf:resource="&uo;Qualification"/>
        <owl:inverseOf rdf:resource="&uo;releases"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#manages -->

    <owl:ObjectProperty rdf:about="&uo;manages">
        <rdfs:comment>A property that associates the university deparment to the degree programme it manages.</rdfs:comment>
        <rdfs:range rdf:resource="&uo;DegreeProgramme"/>
        <rdfs:domain rdf:resource="&uo;Department"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#offers -->

    <owl:ObjectProperty rdf:about="&uo;offers">
        <rdfs:comment>Superproperty that joins an italian public university with a particular educational activity via the degree programme.</rdfs:comment>
        <rdfs:range rdf:resource="&uo;EducationalActivity"/>
        <rdfs:domain rdf:resource="&uo;ItalianPublicUniversity"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&uo;delivers"/>
            <rdf:Description rdf:about="&uo;hasEducationalActivity"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/UniOn#releases -->

    <owl:ObjectProperty rdf:about="&uo;releases">
        <rdfs:comment>A property that relates the degree programme to the official qualification the students receives after completing the study programme.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;DegreeProgramme"/>
        <rdfs:range rdf:resource="&uo;Qualification"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart">
        <rdfs:comment>A property indicating that one entity (the university) includes another entity (the department).</rdfs:comment>
        <rdfs:range rdf:resource="&uo;Department"/>
        <rdfs:domain rdf:resource="&uo;ItalianPublicUniversity"/>
        <owl:inverseOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf">
        <rdfs:comment>A property indicating that one entity (the department) is a part of another entity (the university).</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;Department"/>
        <rdfs:range rdf:resource="&uo;ItalianPublicUniversity"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/ontology/UniOn#hasAcademicYear -->

    <owl:DatatypeProperty rdf:about="&uo;hasAcademicYear">
        <rdfs:comment>The period in which the degree programme carries its traning activity.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;DegreeProgramme"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/UniOn#hasCfu -->

    <owl:DatatypeProperty rdf:about="&uo;hasCfu">
        <rdfs:comment>The CFUs of a particular educational activity. CFUs (university formative credits) are a numerical measure of the workload for each exam or activity.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;EducationalActivity"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/UniOn#hasDegreeClass -->

    <owl:DatatypeProperty rdf:about="&uo;hasDegreeClass">
        <rdfs:comment>The degree class (a unique code) of each degree programme. (E.g. LM-43 - Information technology methods for the humanities)</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;DegreeProgramme"/>
        <rdfs:subPropertyOf rdf:resource="&uo;hasIdentifier"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/UniOn#hasIdentifier -->

    <owl:DatatypeProperty rdf:about="&uo;hasIdentifier">
        <rdfs:comment>A property that expresses the particular identifiers used by the italian university system to describe and identify degree programmes and educational activities. (E.g. Degree Class, Ssd and Teaching Code)</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&uo;DegreeProgramme"/>
                    <rdf:Description rdf:about="&uo;EducationalActivity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/UniOn#hasSsd -->

    <owl:DatatypeProperty rdf:about="&uo;hasSsd">
        <rdfs:comment>The SSD of an educational activity. SSD stands for ‘scientific-disciplinary sectors’: they point to the disciplinary areas to which each educational activity belongs to.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;EducationalActivity"/>
        <rdfs:subPropertyOf rdf:resource="&uo;hasIdentifier"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/UniOn#hasTeachingCode -->

    <owl:DatatypeProperty rdf:about="&uo;hasTeachingCode">
        <rdfs:comment>A numerical code chosen by each university to univocally identify its educational activities.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;EducationalActivity"/>
        <rdfs:subPropertyOf rdf:resource="&uo;hasIdentifier"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/UniOn#hasTeachingLanguage -->

    <owl:DatatypeProperty rdf:about="&uo;hasTeachingLanguage">
        <rdfs:comment>The language in which the degree programme is taught.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;DegreeProgramme"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/UniOn#hasUrl -->

    <owl:DatatypeProperty rdf:about="&uo;hasUrl">
        <rdfs:comment>The Url pointing to the website of the degree programme.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;DegreeProgramme"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/UniOn#hasYearsOfDuration -->

    <owl:DatatypeProperty rdf:about="&uo;hasYearsOfDuration">
        <rdfs:comment>The duration of a degree programme.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;DegreeProgramme"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/UniOn#isCompulsory -->

    <owl:DatatypeProperty rdf:about="&uo;isCompulsory">
        <rdfs:comment>The property indicates if an educational activity is compulsory or not.</rdfs:comment>
        <rdfs:domain rdf:resource="&uo;EducationalActivity"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/ontology/UniOn#AcademicField -->

    <owl:Class rdf:about="&uo;AcademicField">
        <rdfs:comment>The fields involved in an educational offer of a university (e.g. Humanities, Business, Sciences etc.).</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/UniOn#AccessType -->

    <owl:Class rdf:about="&uo;AccessType">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&uo;isAccessTypeOf"/>
                <owl:someValuesFrom rdf:resource="&uo;DegreeProgramme"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The modality of  access the degree courses require.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/UniOn#AssessmentMethod -->

    <owl:Class rdf:about="&uo;AssessmentMethod">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&uo;isAssessmentMethodOf"/>
                <owl:someValuesFrom rdf:resource="&uo;EducationalActivity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The evaluation modality for each educational activity (e.g. Written exam, Oral exam etc.)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/UniOn#BachelorDegree -->

    <owl:Class rdf:about="&uo;BachelorDegree">
        <rdfs:subClassOf rdf:resource="&uo;DegreeProgramme"/>
        <rdfs:comment>The bachelor degree (180 CFU) is a first cycle degree whose normative time to completion is three years.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/UniOn#Course -->

    <owl:Class rdf:about="&uo;Course">
        <rdfs:subClassOf rdf:resource="&uo;EducationalActivity"/>
        <rdfs:comment>A university course is made up of a set of lessons held by one teacher or several teachers.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/UniOn#Curriculum -->

    <owl:Class rdf:about="&uo;Curriculum">
        <rdfs:comment>The same degree program can have multiple curricula that include different  training activities. E.g.  For the degree course in &quot;Literature&quot;, it is possible to choose between the curriculum in Classical or Modern studies.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/UniOn#DegreeProgramme -->

    <owl:Class rdf:about="&uo;DegreeProgramme">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&uo;isManagedBy"/>
                <owl:someValuesFrom rdf:resource="&uo;Department"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&uo;hasPlaceOfTeaching"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&uo;hasCampus"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&uo;hasAccessType"/>
                <owl:onClass rdf:resource="&uo;AccessType"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&uo;isDeliveredBy"/>
                <owl:someValuesFrom rdf:resource="&uo;ItalianPublicUniversity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&uo;releases"/>
                <owl:someValuesFrom rdf:resource="&uo;Qualification"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&uo;hasCurriculum"/>
                <owl:onClass rdf:resource="&uo;Curriculum"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&uo;hasAcademicField"/>
                <owl:someValuesFrom rdf:resource="&uo;AcademicField"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&uo;hasEducationalActivity"/>
                <owl:onClass rdf:resource="&uo;EducationalActivity"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A university-specific degree programme referring to a more general academic field (e.g. Digital Humanities and Digital Knowledge degree programme at UniBo)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/UniOn#Department -->

    <owl:Class rdf:about="&uo;Department">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&uo;manages"/>
                        <owl:someValuesFrom rdf:resource="&uo;DegreeProgramme"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf"/>
                        <owl:someValuesFrom rdf:resource="&uo;ItalianPublicUniversity"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Organizational structure that within Italian universities promotes and coordinates the activities of one or more sectors of scientific research that are homogeneous in terms of aims and methods.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/UniOn#EducationalActivity -->

    <owl:Class rdf:about="&uo;EducationalActivity">
        <rdfs:comment>Any kind of educational activity: exams, internships, laboratories, seminars etc.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/UniOn#FinalTest -->

    <owl:Class rdf:about="&uo;FinalTest">
        <rdfs:subClassOf rdf:resource="&uo;EducationalActivity"/>
        <rdfs:comment>The final examination: it consists in producing a dissertation on a topic that is consistent with the learning objectives of the degree programme, drafted in an original manner under the guidance of a supervising professor</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/UniOn#InternationalDegree -->

    <owl:Class rdf:about="&uo;InternationalDegree">
        <rdfs:subClassOf rdf:resource="&uo;DegreeProgramme"/>
        <rdfs:comment>A degree programme delivered in a foreign language.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/UniOn#Internship -->

    <owl:Class rdf:about="&uo;Internship">
        <rdfs:subClassOf rdf:resource="&uo;EducationalActivity"/>
        <rdfs:comment>An internship is a period of work experience offered by an organization or an institution for a limited period of time.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/UniOn#ItalianPublicUniversity -->

    <owl:Class rdf:about="&uo;ItalianPublicUniversity">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&uo;delivers"/>
                        <owl:onClass rdf:resource="&uo;DegreeProgramme"/>
                        <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart"/>
                        <owl:onClass rdf:resource="&uo;Department"/>
                        <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>A specific public italian university (e.g. Unibo)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/UniOn#Laboratory -->

    <owl:Class rdf:about="&uo;Laboratory">
        <rdfs:subClassOf rdf:resource="&uo;EducationalActivity"/>
        <rdfs:comment>A laboratory deals with specific topics related to the degree programme and involves practical activities.</rdfs:comment>
    </owl:Class>
    


