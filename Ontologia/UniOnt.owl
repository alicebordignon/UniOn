<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#"
     xml:base="http://www.semanticweb.org/francesca/ontologies/2022/6/uni"
     xmlns:dul="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:uni="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni">
        <rdfs:comment>A marvellous ontology about Italian Universities&apos; formative offer :)</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart">
        <owl:inverseOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#ItalianPublicUniversity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#Department"/>
        <rdfs:comment>Links UNIVERSITY w/ DEPARTMENT</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf"/>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#delivers -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#delivers">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isDeliveredBy"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#ItalianPublicUniversity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#DegreeProgramme"/>
        <rdfs:comment>Links Public Italian UNIVERSITY with the DEGREE PROGRAMMES it delivers</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasAcademicField -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasAcademicField">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isAcademicFieldOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasAccessType -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasAccessType">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isAccessTypeOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasAssessmentMethod -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasAssessmentMethod">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#EducationalActivity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#AssessmentMethod"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasCampus -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasCampus">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#DegreeProgramme"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasCurriculum -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasCurriculum">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isCurriculumOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasEducationalActivity -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasEducationalActivity">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isEducationalActivityOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasPlaceOfTeaching -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasPlaceOfTeaching">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#DegreeProgramme"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place"/>
        <rdfs:comment>Links DEGREE PROGRAMME and PLACE</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isAcademicFieldOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isAcademicFieldOf"/>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isAccessTypeOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isAccessTypeOf"/>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isAssessmentMethodOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isAssessmentMethodOf"/>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isCurriculumOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isCurriculumOf"/>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isDeliveredBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isDeliveredBy"/>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isEducationalActivityOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isEducationalActivityOf"/>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isManagedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isManagedBy">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#manages"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isOfferedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isOfferedBy">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#offers"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#manages -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#manages">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#Department"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#DegreeProgramme"/>
        <rdfs:comment>Links DEPARTMENT to DEGREE PROGRAMME</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#offers -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#offers">
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasEducationalActivity"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#delivers"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>Superproperty that joins the UNIVERSITY with a given EXAM via the the degree programme.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#releases -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#releases">
        <rdfs:comment>Links DEGREE PROGRAMME and QUALIFICATION</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasAcademicYear -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasAcademicYear">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasCfu -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasCfu">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasDegreeClass -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasDegreeClass">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasIdentifier"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasIdentifier -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasIdentifier"/>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasSsd -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasSsd">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasIdentifier"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasTeachingCode -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasTeachingCode">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasIdentifier"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasTeachingLanguage -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasTeachingLanguage">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#language"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasUrl -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasUrl">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasYearsOfDuration -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasYearsOfDuration">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isCompulsory -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isCompulsory">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#EducationalActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#label -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#label">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place">
        <rdfs:comment>Place where the educational activity takes place. </rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#AcademicField -->

    <owl:Class rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#AcademicField">
        <rdfs:comment>The fields involved in an educational offer of a university (Humanities, Business, Sciences etc.).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#AccessType -->

    <owl:Class rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#AccessType">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isAccessTypeOf"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#DegreeProgramme"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#AssessmentMethod -->

    <owl:Class rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#AssessmentMethod">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isAssessmentMethodOf"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#EducationalActivity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The evaluation modality for each educational activity (e.g. Written exam, Oral exam etc.) </rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#BachelorDegree -->

    <owl:Class rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#BachelorDegree">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#DegreeProgramme"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#Course -->

    <owl:Class rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#Course">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#EducationalActivity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasSsd"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasTeachingCode"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#Curriculum -->

    <owl:Class rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#Curriculum">
        <rdfs:comment>E.g. Literature divided in Classical and Modern studies</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#DegreeProgramme -->

    <owl:Class rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#DegreeProgramme">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasAcademicField"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#AcademicField"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasCampus"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasPlaceOfTeaching"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isDeliveredBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#ItalianPublicUniversity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#isManagedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#Department"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#releases"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#Qualification"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasCurriculum"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#Curriculum"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasEducationalActivity"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#EducationalActivity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasAccessType"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#AccessType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasUrl"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasYearsOfDuration"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#hasDegreeClass"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A university-specific degree programme referring to a more general academic field (e.g. Digital Humanities and Digital Knowledge degree programme at UniBo)</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/uni#Department -->

    <owl:Class rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#Department">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#ItalianPublicUniversity"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#manages"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/uni#DegreeProgramme"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Organizational structure that within Italian universities promotes and coordinates the activities of one or more sectors of scientific research that are homogeneous in terms of aims and methods.</rdfs:comment>
