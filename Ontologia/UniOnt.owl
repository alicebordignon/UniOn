    <!-- http://www.semanticweb.org/francesca/ontologies/2022/6/untitled-ontology-24#AcademicField -->

    <owl:Class rdf:about="http://www.semanticweb.org/francesca/ontologies/2022/6/untitled-ontology-24#AcademicField">
