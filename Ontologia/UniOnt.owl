                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/untitled-ontology-24#ItalianPublicUniversity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/untitled-ontology-24#isManagedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/francesca/ontologies/2022/6/untitled-ontology-24#Department"/>
